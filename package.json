{
  "name": "darl",
  "version": "3.0.0",
  "description": "Run npm scripts in parallel",
  "main": "lib.js",
  "type": "commonjs",
  "types": "lib.d.ts",
  "typings": "lib.d.ts",
  "exports": {
    ".": {
      "import": "./lib.mjs",
      "require": "./lib.js"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^8.1.0",
    "ink": "^3.0.9",
    "react": "^16.14.0"
  },
  "devDependencies": {
    "@types/node": "^16.4.10",
    "@types/react": "^17.0.15",
    "symlink-dir": "^5.0.1",
    "typescript": "^4.3.5"
  },
  "scripts": {
    "tsc:w": "tsc -w",
    "tsc": "tsc",
    "link": "npm link --force",
    "init": "symlink-dir . node_modules/darl",
    "darl": "darl",
    "test:echo": "powershell -command echo 123"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/volight/darl"
  },
  "keywords": [
    "dev",
    "parallel"
  ],
  "author": "Volight",
  "license": "MIT",
  "bin": {
    "darl": "./darl.js"
  },
  "files": [
    "darl.js",
    "run.js",
    "lib.js",
    "lib.mjs",
    "lib.d.ts",
    "LICENSE"
  ]
}
